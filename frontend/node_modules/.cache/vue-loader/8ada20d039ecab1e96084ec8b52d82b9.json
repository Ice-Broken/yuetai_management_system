"primary\" icon=\"el-icon-check\" @click=\"create\">添加大采购商</el-button>\n  </el-form>\n</template>\n<script setup>\nimport { ref } from \"vue\";\nimport { ElMessage } from \"element-plus\";\nimport createBBuyer from \"@/modules/teacher/create-b_buyer\";\n\nlet bbuyer_name_ch = ref(\"\");\nlet bbuyer_name_en = ref(\"\");\nlet address = ref(\"\");\nlet fax = ref(\"\");\nlet contact_name = ref(\"\");\nlet position = ref(\"\");\nlet tel = ref(\"\");\nlet email = ref(\"\");\nlet remark = ref(\"\");\n\nfunction create() {\n  // let telTest = /^(?:(?:\\+|00)86)?1[3-9]\\d{9}$/;\n  let nullTest = /^[\\s\\S]*.*[^\\s][\\s\\S]*$/;\n  let isCreate = nullTest.test(bbuyer_name_ch.value);\n  if (isCreate) {\n    createBBuyer({\n      bbuyer_name_ch: bbuyer_name_ch.value,\n      bbuyer_name_en: bbuyer_name_en.value,\n      address: address.value,\n      fax: fax.value,\n      contact_name: contact_name.value,\n      position: position.value,\n      tel: tel.value,\n      email: email.value,\n      remark: remark.value,\n    }).then((res) => {\n      ElMessage({\n        message: `成功添加一条新的大采购商：${bbuyer_name_ch.value} 的记录`,\n        type: \"success\",\n      });\n      // name.value = \"\";\n      // tel.value = \"\";\n      // dtm.value = \"\";\n    });\n  } else {\n    ElMessage.warning({\n      message: \"请按照正确格式填写全部内容\",\n      type: \"warning\",\n    });\n  }\n}\n</script>\n\n<style scoped lang='scss'>\n.el-form-item {\n  width: 400px;\n  margin: 0px auto;\n  margin-top: 30px;\n}\n\n.el-button {\n  width: 400px;\n  margin: 0px auto;\n  margin-top: 30px;\n  display: block;\n}\n\n.el-select {\n  margin-top: 0px !important;\n  margin-left: 0px !important;\n}\n</style>"]}]}