n((res) => {\n    start();\n  });\n}\n</script>\n<style scoped lang='scss'>\n.head {\n  text-align: center;\n  margin-top: 30px;\n}\nh2 {\n  text-align: center;\n}\n.box {\n  text-align: center !important;\n}\n.el-button {\n  width: 220px;\n  margin: 0px auto !important;\n  display: block;\n}\n</style>\n"]}]}